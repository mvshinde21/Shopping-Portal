package com.project.testcases;

import org.testng.annotations.Test;
import p_pageobjectmodel.example1.tests.RegistrationPage;
import org.testng.annotations.BeforeClass;
import org.testng.annotations.DataProvider;
import org.testng.annotations.AfterClass;
public class RegistrationPageTest {
	
  RegistrationPage regObj;
  @Test(priority=1,dataProvider="RegistrationData")
  public void registration(String name,String email,String password,String contact,String cp) {
	  regObj.RegistrationWithValidEmail(name,email,password,contact,cp);  
	 
  }
  @Test(priority=2,dataProvider="RegistrationData")
  public void reRegistration(String name,String email,String password,String contact,String cp)
  {
	  regObj.RegistrationWithValidEmail(name,email,password,contact,cp); 
	  
  }
  @DataProvider(name="RegistrationData")
  public String [][] getData()
  
  {
	  String RegistrationData[][]= {
			  {"nirml","abcdefgg@yahoo.com","1234","12345","12345"}, 
			  
   };
	  return RegistrationData;
  }
  @BeforeClass
  public void beforeClass() {
	  regObj=new RegistrationPage();
	  regObj.initializeBrowser();
	  regObj.driver.get(regObj.baseUrl);
	  
  }
  @AfterClass
  public void afterClass() {
	  regObj.closeBrowser();  
  }
}
